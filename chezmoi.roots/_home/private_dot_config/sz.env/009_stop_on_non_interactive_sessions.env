#!/usr/bin/env -S bash -c 'echo "Not a user script. source(aka .) only"'

if [[ ! "$-" =~ i ]]; then
    LOAD_EXIT=1
    echo "Non interactive session ($-) , stopping load of shell environment" 1>&2
else
    ${NO_RESET:+:} reset
    export NO_RESET=1

    [ -z "$UMASK" ] || umask "$UMASK"
    export TERM="${TERM:-xterm-256color}"
    export LANG="${LANG:-en_US.UTF-8}"
fi

