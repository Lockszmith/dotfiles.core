#!/usr/bin/env -S bash -c 'echo "Not a user script. source(aka .) only"'

export ENVMAN_LOAD=''

if [ -s "$HOME/.config/envman/load.sh" ]; then
    [[ -n "${DBG}" ]] && echo "Loading envman"
    source "$HOME/.config/envman/load.sh"

    [[ -n "${DBG}" ]] && echo "${PATH//:/\n}"
fi

if is_cmd webi; then
    . <(webi --init "${SHELL##*/}")
fi

