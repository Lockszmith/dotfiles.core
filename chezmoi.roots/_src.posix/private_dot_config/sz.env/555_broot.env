#!/usr/bin/env -S bash -c 'echo "Not a user script. source(aka .) only"'

if is_cmd broot; then
    . <(broot --print-shell-function "${SHELL##*/}")
    [[ -n "$( printf '%s' ~/.config/broot/launcher/installed* )" ]] 2> /dev/null \
    || broot --set-install-state installed
fi

